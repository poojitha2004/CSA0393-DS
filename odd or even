#include<stdio.h>
int main()
{
int n,a[20],i,even =0,odd=0;
printf("enter the number of elements");
scanf("%d",&n);
printf("enter the elements in array");
printf("\n");
for(i=0;i<n;i++)
{
	printf("enter the element a[%d]",i);
	scanf("%d",&a[i]);
}
printf("\nthe odd elements are\n");
for(i=0;i<n;i++)
{
	if(a[i]%2!=0)
	{
		printf("%d\n",a[i]);
	}
}
printf("\n enter the even elements are:");
for(i=0;i<n;i++)
{
	if(a[i]%2==0)
	{
		printf("%d\n",a[i]);
	}
}
return 0;
}
